<?xml version="1.0" encoding="utf-8"?>
<npcAi>
	<!--예약-->
	<reserved>
		<condition name="hpLess" value="101">
			<!--1페이즈  -->
			<!--전투 활성화 되면 최초 딱 한번만 실행함-->
			<node name="standby" limit="1100" faceTarget="1" isKeepBattle="true">
				<node name="SetValue" key="Phase" value="1" />
				<!-- 1페이즈 -->
				<node name="SetValue" key="AttackDelay" value="0" />
				<!-- 공격 후 standby 행동을 하는데 사용하는 변수-->
				<node name="SetValue" key="JumpPosition" value="0" />
				<node name="SetValue" key="JumpNumber" value="0" />
				<node name="SetValue" key="AttackNumber" value="0" />
			</node>
		</condition>
		<condition name="hpLess" value="80">
			<!--2페이즈  -->
			<node name="SetValue" key="Phase" value="2">
				<!-- 2페이즈 -->
				<node name="SetValue" key="SpraykMode" value="0" />
				<!-- 이 변수 숫자 넣어 스프레이 공격 패턴 시작, 이 변수 3 넣으면 스프레이 공격 2번 함, 예: 이 변수 4 넣으면 스프레이 공격 3번 함 -->
			</node>
		</condition>
		<condition name="hpLess" value="75">
			<!--3페이즈 -->
			<node name="SetValue" isKeepBattle="true" key="Phase" value="3">
				<!-- 3페이즈 -->
				<node name="SetValue" key="PatternSlave" value="1" />
				<!-- Master인 핑크빈에게 받는 신호 변수, 최초 숫자 설정은 1로 하기 -->
				<node name="SetValue" key="VacuumNumber" value="1" />
				<!-- 이 변수는 초기값 1, 스킬 레벨과 숫자 동일하게 하기 위해 -->
				<node name="SetValue" key="JumpNumber" value="0" />
				<node name="SetValue" key="AttackDelay" value="0" />
				<!-- 페이즈로 넘어가면 바로 행동하도로 하기 위해 이 변수 0 설정-->
				<node name="SetValue" key="AttackNumber" value="0" />
				<node name="SetValue" key="SafeSafe" value="0" />
				<!-- 3페이즈에서 신호 꼬였을 때를 대비하는 안전장치용 변수 -->
			</node>
		</condition>
		<condition name="hpLess" value="38">
			<!--4페이즈 , 두번째 전투판으로 점프이동하기-->
			<node name="SetValue" isKeepBattle="true" key="Phase" value="4">
				<!-- 4페이즈 -->
				<node name="skill" facePos="1950, 1950, 750" idx="4" />
				<!-- 점프 도약 -->
				<node name="jump" pos="1950, 1950, 750" speed="1300" heightMultiplier="0.3" />
				<!--점프중 -->
				<node name="skill" faceTarget="0" idx="5" />
				<!-- 점프 착지 -->
				<node name="SetValue" key="JumpNumber" value="0" />
				<node name="SetValue" key="AttackNumber" value="0" />
				<node name="SetValue" key="AttackDelay" value="0" />
				<!-- 페이즈로 넘어가면 바로 행동하도로 하기 위해 이 변수 0 설정-->
				<node name="standby" limit="2100" faceTarget="1" />
			</node>
		</condition>
		<condition name="hpLess" value="21">
			<!--5페이즈 -->
			<node name="SetValue" isKeepBattle="true" key="Phase" value="5">
				<!-- 5페이즈 -->
				<node name="SetValue" key="AttackDelay" value="0" />
				<!-- 페이즈로 넘어가면 바로 행동하도로 하기 위해 이 변수 0 설정-->
			</node>
		</condition>
	</reserved>
	<!--전투-->
	<battle>
		<node name="conditions">
			<condition name="extraData" key="AttackDelay" value="1" op="greaterEqual">
				<!-- 공격 후 standby 상태 -->
				<node name="standby" limit="100" faceTarget="1">
					<node name="SetValue" key="AttackDelay" value="-1" isModify="1" />
				</node>
			</condition>
			<condition name="extraData" key="Phase" value="1">
				<!--1페이즈:  첫번째 전투판 여기저기 점프이동하기  -->
				<node name="conditions">
					<condition name="extraData" key="JumpNumber" value="2" op="lessEqual">
						<aiPreset name="JumpMovePattern_1st" />
					</condition>
					<condition name="extraData" key="AttackNumber" value="6" op="lessEqual">
						<node name="select" prob="9999,99999,0">
							<!-- 몇번 이상 하면 공격 행동 하면 다음 단계로 넘어감 -->
							<node name="target" type="near" from="10" to="1800" cooltime="2400" />
							<!-- 제일 가까운 대상 타겟팅 하기 -->
							<node name="standby" limit="120" faceTarget="1" cooltime="33400">
								<node name="conditions">
									<condition name="hpOver" value="93">
										<node name="target" type="near" from="10" to="1800" />
										<!--HP가 93% 이상이면 아무것도 안함  -->
									</condition>
									<condition name="true">
										<!--HP가 93%~80% 이면 스프레이 공격 자주 하기  -->
										<node name="skill" faceTarget="1" isKeepBattle="true" idx="20">
											<!-- 스프레이 뿌리기 준비 -->
											<node name="skill" faceTarget="0" idx="21" />
											<node name="target" type="rand" from="10" to="2500" />
											<node name="skill" faceTarget="1" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="skill" faceTarget="0" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="target" type="rand" from="10" to="2500" />
											<node name="skill" faceTarget="1" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="skill" faceTarget="0" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="skill" faceTarget="0" idx="22" />
											<node name="SetValue" key="AttackNumber" value="8" />
											<!-- 스프레이 공격 후 바로 점프단계로 넘어가도록 하기 -->
											<node name="SetValue" key="AttackDelay" value="0" />
										</node>
									</condition>
								</node>
							</node>
							<node name="conditions">
								<condition name="skillRange" skillIdx="31">
									<node name="select" prob="99999,45,27,28">
										<!-- 레벨2: 거리 550, 플레이어가 가까이 붙으면 딜하기 더 좋게 하기 위한 설정-->
										<node name="standby" limit="120" faceTarget="1" cooltime="2400">
											<node name="select" prob="80,20">
												<node name="SetValue" key="AttackNumber" value="1" isModify="1" />
												<node name="SetValue" cooltime="15400" key="AttackNumber" value="2" isModify="1" />
											</node>
										</node>
										<node name="standby" limit="120" faceTarget="1">
											<node name="conditions">
												<condition name="skillRange" skillIdx="32">
													<!-- 레벨2: 거리 420 이내에 있으면 제자리 휘두르기-->
													<node name="skill" faceTarget="1" isKeepBattle="true" idx="32">
														<!-- 붓 휘두르기, 레벨2 제자리 휘두르기 -->
														<node name="SetValue" key="AttackDelay" value="18" />
													</node>
												</condition>
												<condition name="true">
													<!-- 420~550 사이에 있으면 돌진 휘두르기-->
													<node name="skill" faceTarget="1" isKeepBattle="true" idx="31">
														<!-- 붓 휘두르기, 레벨1 돌진 휘두르기 -->
														<node name="SetValue" key="AttackDelay" value="22" />
													</node>
												</condition>
											</node>
										</node>
										<node name="skill" faceTarget="1" cooltime="12000" isKeepBattle="true" idx="2">
											<!-- 사방 발사 바닥 얼굴 폭발, 레벨2은 자동차 탑승 상태 아님 -->
											<node name="SetValue" key="AttackDelay" value="20" />
										</node>
										<node name="skill" faceTarget="1" cooltime="15000" isKeepBattle="true" idx="1">
											<!-- 얼굴 투사체 발사, 레벨3 상태이상 없음 -->
											<node name="SetValue" key="AttackDelay" value="19" />
										</node>
									</node>
								</condition>
								<condition name="skillRange" skillIdx="1">
									<node name="select" prob="99999,34,33,32,1">
										<!-- 거리 1400, 먼 거리이ㅔ 있으면 더 자주 공격함 -->
										<node name="standby" limit="120" faceTarget="1" cooltime="3000">
											<node name="select" prob="1,99">
												<node name="SetValue" key="AttackNumber" value="0" isModify="1" />
												<node name="SetValue" key="AttackNumber" value="1" isModify="1" />
											</node>
										</node>
										<node name="skill" faceTarget="1" cooltime="6000" isKeepBattle="true" idx="3">
											<!-- 브러쉬 거대 투사체, 레벨2가 자동타 탑승 아닌 상태에서 사용하는 스킬임 -->
											<node name="SetValue" key="AttackDelay" value="17" />
										</node>
										<node name="skill" faceTarget="1" cooltime="6000" isKeepBattle="true" idx="1">
											<!-- 얼굴 투사체 발사, 레벨3은 상태이상 없음-->
											<node name="SetValue" key="AttackDelay" value="21" />
										</node>
										<node name="skill" faceTarget="1" cooltime="39500" initialCooltime="9000" isKeepBattle="true" idx="20">
											<!-- 스프레이 뿌리기 준비 -->
											<node name="skill" faceTarget="0" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="skill" faceTarget="1" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="target" type="rand" from="10" to="2500" />
											<node name="skill" faceTarget="1" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="skill" faceTarget="0" idx="25" />
											<!-- 레벨24: 상태이상 없음 400 이동-->
											<node name="skill" faceTarget="0" idx="22" />
											<node name="SetValue" key="AttackNumber" value="8" />
											<!-- 스프레이 공격 후 바로 점프단계로 넘어가도록 하기 -->
											<node name="SetValue" key="AttackDelay" value="19" />
										</node>
										<!-- 위 스킬 쿨타임 걸리면 이거 사용함: 얼굴 투사체 발사, 레벨3은 상태이상 없음 -->
										<node name="skill" faceTarget="1" isKeepBattle="true" idx="1">
											<node name="SetValue" key="AttackDelay" value="19" />
										</node>
									</node>
								</condition>
								<condition name="true">
									<node name="standby" limit="260" faceTarget="1" isKeepBattle="true">
										<node name="target" type="near" from="5" to="1800" />
										<node name="SetValue" key="AttackNumber" value="1" isModify="1" />
									</node>
								</condition>
							</node>
						</node>
					</condition>
					<condition name="true">
						<node name="standby" limit="250" faceTarget="1" isKeepBattle="true">
							<!-- 변수 초기화 -->
							<node name="SetValue" key="JumpNumber" value="0" />
							<node name="SetValue" key="AttackNumber" value="0" />
						</node>
					</condition>
				</node>
			</condition>
			<condition name="extraData" key="Phase" value="2">
				<!--2페이즈:  83~73% -->
				<aiPreset name="CombineAttack_Main" />
			</condition>
			<condition name="extraData" key="Phase" value="3">
				<!--3페이즈:  73~35% -->
				<node name="conditions">
					<condition name="extraData" key="PatternSlave" value="1">
						<node name="select" prob="9999,1">
							<!-- 증요!!: 이  JumpNumber AttackNumber 두변수는 여기저기서 자주 사용되기 때문에 이 페이즈의 시작단계에서 꼭 0 초기화 해야함!! -->
							<node name="SetValue" cooltime="4970" key="JumpNumber" value="0">
								<node name="SetValue" key="AttackNumber" value="0">
									<node name="SetValue" key="SafeSafe" value="0" />
									<!-- 증요!!: 변수 신호 꼬였을 때를 대비하는 안전장치용 변수도 0 초기화 -->
								</node>
							</node>
							<node name="SetValue" key="PatternSlave" value="3">
								<aiPreset name="RolingAttack_Main" />
							</node>
						</node>
					</condition>
					<condition name="extraData" key="PatternSlave" value="3">
						<node name="select" prob="99,1">
							<!--  PatternSlave = 3는 핑크빈 AI로 부터 PatternSlave 변수 4 혹은 5 신호 받을때까지 standby 하거나 점프뛰기  -->
							<node name="standby" limit="1100" faceTarget="1" cooltime="3100" />
							<node name="standby" limit="230" faceTarget="1">
								<aiPreset name="JumpMovePattern_1st_Mini" />
							</node>
						</node>
					</condition>
					<condition name="extraData" key="PatternSlave" value="4">
						<!--  PatternSlave = 4는 블랙빈 진공청소기 공격하기  -->
						<aiPreset name="VacuumAttack" />
					</condition>
					<condition name="extraData" key="PatternSlave" value="5">
						<node name="select" prob="9999,99,1">
							<!--  PatternSlave = 5는 블랙빈이 요요 공격할 때 맵 구석으로 가서 대기상태로 있기  -->
							<node name="skill" facePos="-1200, -150, 1650" cooltime="81000" idx="4">
								<!--레벨1 일반 점프 도약 -->
								<node name="jump" pos="-1200, -150, 1650" speed="1500" heightMultiplier="0.6">
									<!--점프뛰기-->
									<node name="skill" faceTarget="0" idx="5" />
									<!--레벨2: 점프 착지 -->
								</node>
							</node>
							<node name="target" type="near" from="5" to="1800" cooltime="2300" />
							<node name="standby" limit="1100" faceTarget="1" />
							<!-- 핑크빈 AI로 부터  PatternSlave = 6 신호 받을때까지 대기하기 -->
						</node>
					</condition>
					<condition name="extraData" key="PatternSlave" value="60" op="lessEqual">
						<!--PatternSlave = 6~60, AI_Pinkbean_Together_Hard.xml AI_Pinkbean_Together_Normal.xml 에서 신호 받음-->
						<node name="SetValue" key="PatternSlave" value="1" isModify="1">
							<node name="conditions">
								<condition name="extraData" key="JumpNumber" value="3" op="lessEqual">
									<aiPreset name="JumpMovePattern_1st" />
								</condition>
								<condition name="extraData" key="AttackNumber" value="2" op="lessEqual">
									<!-- 몇번 이상 하면 공격 행동 하면 다음 단계로 넘어감 -->
									<aiPreset name="NormalAttackPattern_2Phase" />
								</condition>
								<condition name="true">
									<node name="SetValue" key="JumpNumber" value="0">
										<node name="SetValue" key="AttackNumber" value="0" />
									</node>
								</condition>
							</node>
						</node>
					</condition>
					<condition name="true">
						<!--  PatternSlave = 61 이상, 핑크빈 블랙빈 중 먼저 이 던계에 오는 몬스터가 AI에서 신호 보내서 처음 단계로 가도록 함 -->
						<node name="standby" limit="270" faceTarget="1">
							<node name="SetValue" key="PatternSlave" value="1">
								<node name="SetMasterValue" key="PatternMaster" value="1" />
							</node>
						</node>
					</condition>
				</node>
			</condition>
			<condition name="true">
				<!--  4~5페이즈: 두번째 전투판으로 이동 RGB 공격 패턴 -->
				<node name="conditions">
					<condition name="skillRange" skillIdx="33">
						<node name="select" prob="99999,0">
							<!-- 사정거리 1300, 높이 250 -->
							<node name="standby" limit="200" faceTarget="1" cooltime="45000">
								<node name="select" prob="33,34,33">
									<!-- 전투판 바닥 전체 RGB 설치하기 -->
									<node name="say" message="$Blackbean_Paint01$" durationTick="8000" cooltime="115000">
										<node name="skill" facePos="-2100, -2100, -1800" idx="34" />
									</node>
									<node name="say" message="$Blackbean_Paint02$" durationTick="8000" cooltime="115000">
										<node name="skill" facePos="-2100, -2100, -1800" idx="35" />
									</node>
									<node name="skill" facePos="-2100, -2100, -1800" cooltime="115000" idx="36" />
								</node>
							</node>
							<aiPreset name="NormalAttackPattern_Final" />
						</node>
					</condition>
					<condition name="true">
						<node name="standby" limit="500" faceTarget="1">
							<node name="target" type="near" from="5" to="1800" />
						</node>
					</condition>
				</node>
			</condition>
		</node>
	</battle>
	<battleEnd onlyDead="true">
		<!--죽을 때 선언하는 부분, bossSpawn.xml   Barricade.xml   GiveUpTime.xml 에 신호 보내기-->
		<node name="TriggerSetUserValue" triggerID="529" key="BattleEnd" value="1">
			<!-- bossSpawn.xml  에 신호 보내기-->
			<node name="TriggerSetUserValue" triggerID="5291" key="BattleEnd" value="1">
				<!-- Barricade.xml  에 신호 보내기-->
				<node name="TriggerSetUserValue" triggerID="5292" key="BattleEnd" value="1" />
				<!-- GiveUpTime.xml 에 신호 보내기-->
			</node>
		</node>
	</battleEnd>
	<aiPresets>
		<aiPreset name="JumpMovePattern_1st">
			<node name="select" prob="99999,0">
				<node name="standby" limit="120" faceTarget="1" cooltime="2000">
					<node name="select" prob="50,50">
						<node name="SetValue" cooltime="14500" key="JumpNumber" value="1" isModify="1" />
						<node name="SetValue" key="JumpNumber" value="2" isModify="1" />
					</node>
				</node>
				<node name="conditions">
					<condition name="extraData" key="JumpPosition" value="1">
						<node name="select" prob="50,50">
							<!-- 현재 위치 두번째 전투판 기준 1시쪽 저점에 있을 경우 -->
							<node name="skill" facePos="-2850, -2250, 1350" isKeepBattle="true" idx="4">
								<!--7시 지점으로 점프 -->
								<node name="jump" pos="-2850, -2250, 1350" speed="1300" heightMultiplier="0.3" />
								<!--점프중 -->
								<node name="skill" faceTarget="0" idx="6" />
								<!--점프 착지, 레벨3은 바닥 얼굴 대미지 필드 공격 -->
								<node name="SetValue" key="JumpPosition" value="7" />
								<!--1층 전투판 7시 지점에 있음을 알림 -->
								<node name="SetValue" key="AttackDelay" value="9" />
								<!-- standby 동작 길이 설정 -->
							</node>
							<node name="skill" facePos="-2250, -900, 1350" isKeepBattle="true" idx="4">
								<!--10시 지점으로 점프 -->
								<node name="jump" pos="-2250, -900, 1350" speed="1300" heightMultiplier="0.3" />
								<!--점프중 -->
								<node name="skill" faceTarget="0" idx="6" />
								<!--점프 착지, 레벨3은 바닥 얼굴 대미지 필드 공격 -->
								<node name="SetValue" key="JumpPosition" value="10" />
								<!--1층 전투판 10시 지점에 있음을 알림 -->
								<node name="SetValue" key="AttackDelay" value="8" />
								<!-- standby 동작 길이 설정 -->
							</node>
						</node>
					</condition>
