				<node name="TriggerSetUserValue" triggerID="553" key="MonsterMany" value="0" />
				<!-- TriggerModelID =  553 -->
				<node name="buff" type="remove" id="50003307" />
			</node>
		</condition>
		<condition name="hpLess" value="60">
			<!-- 독바닥 패턴 HP 60~40%, 페이즈 전환 변수 설정은 여기서 안함-->
			<!-- @@ 3페이즈 전환은 <aiPreset name="VacuumAttack"/>  여기서 이루어 지도록-->
			<node name="buff" type="remove" id="50003307" />
