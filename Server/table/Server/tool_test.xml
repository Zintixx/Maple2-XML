<?xml version="1.0" encoding="utf-8"?>
<ms2>
	<cmd keyword="cdch" cmd_type="1" name="Creating and/or Deleting Characters" help="cdch /count:[value] /mode:[1:create|2:delete|3:random] \ncdch /stop">
		<option keyword="/count" default="5000" />
		<option keyword="/mode" default="1" />
	</cmd>
	<cmd keyword="udf" cmd_type="2" name="Updating Dummy Fields" help="udf /field:[field&apos;s id] /count:[value] \nudf /stop">
		<option keyword="/field" default="2000001" />
		<option keyword="/count" default="1000" />
	</cmd>
	<cmd keyword="ruch" cmd_type="2" name="Reading and/or Updating Dummy Users" help="ruch /count:[value] /timeout.min:[sec] /timeout.max:[sec] /lifetime.min:[sec] /lifetime.max:[sec] \nruch /stop">
		<option keyword="/count" default="1000" />
		<option keyword="/timeout.min" default="10" />
		<option keyword="/timeout.max" default="30" />
		<option keyword="/lifetime.min" default="60" />
		<option keyword="/lifetime.max" default="120" />
	</cmd>
	<cmd keyword="perf" cmd_type="3" name="Toggle the Performance Monitor" help="perf [to start or stop]" />
</ms2>